                    <protocExecutable>D:\protobuff\forCpp\protoc-3.2.0-win32\bin\protoc.exe</protocExecutable>
                </configuration>
            </plugin> -->

            <!-- <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin> -->


        </plugins>


    </build>

</project>
